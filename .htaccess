RewriteEngine On

RewriteRule ^/$ index.php?url=/ [L]
RewriteRule ^admin$ index.php?url=admin [L]

RewriteRule ^admin/dang-nhap$ index.php?url=admin/dang-nhap [L]
RewriteRule ^admin/quen-mat-khau$ index.php?url=admin/quen-mat-khau [L]

RewriteRule ^test/([0-9]+)/([0-9]+)/([A-Za-z0-9-]+)$ index.php?url=test&id=$1&slug=$2&slug2=$3 [L]

RewriteRule ^admin/ve$ index.php?url=admin/ve [L]
RewriteRule ^admin/them-ve$ index.php?url=admin/them-ve [L]

RewriteRule ^admin/hang-hang-khong$ index.php?url=admin/hang-hang-khong [L]

RewriteRule ^admin/san-bay$ index.php?url=admin/san-bay [L]

RewriteRule ^san-pham$ index.php?url=san-pham [L]
RewriteRule ^chi-tiet-san-pham/([0-9]+)$ index.php?url=chi-tiet-san-pham&id=$1 [L]

RewriteRule ^404$ index.php?url=404 [L]


ErrorDocument 404 http://localhost/my-project/asm_php2_route/404